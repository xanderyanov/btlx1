mixin eCatalog 
  //- импорт из data.json (подключается через gulp task)
  each item in eCatalogBase
    .eCatalog__itemArea(id=item.id)
      .eCatalog__item(href="")
        .eCatalog__itemFav
          i.icon-eheart1.iconNoFav
          i.icon-eheart1.iconInFav
        a.eCatalog__itemLink(href="") 
          .eCatalog__itemImgOuter 
            .eCatalog__itemImg(style='background-image: url(' + item.img + ');')
              if item.oldPrice
                .eCatalog__itemDiscount!= item.discount 
          .eCatalog__itemInfo 
            .eCatalog__itemTitle!= item.title 
            .eCatalog__itemDesc!= item.content 
        .eCatalog__itemPriceArea 
          .eCatalog__itemPriceBox
            if item.oldPrice
              .eCatalog__itemOldPrice
                span!= item.oldPrice 
                span ₽  
            .eCatalog__itemPrice
              span!= item.price 
              span ₽
          .eCatalog__itemByBtn 
            i.icon-eshopping-cart


